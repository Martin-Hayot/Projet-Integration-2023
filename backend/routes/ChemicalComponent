const  express = require("express")
const router = express.Router()
const ChemicalComponentController = require('../controllers/ChemicalComponent')
const authentification = require('../middelware/authentification')

router.post('/createAquarium', ChemicalComponentController.createChemicalComponent);

router.get('/showAllUsers', authentification, ChemicalComponentController.showAllChemicalComponent)

//router.get('/showOneUser', ChemicalComponentController.ShowOneChemicalComponent);

router.put('/modify/:id', ChemicalComponentController.modifyChemicalComponent);

router.delete('/delete/:id', authentification, ChemicalComponentController.deleteChemicalComponent);

module.exports = router